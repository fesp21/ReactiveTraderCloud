{
  "name": "reactive-trader-cloud",
  "version": "2.1.0",
  "description": "Reactive Trader - Cloud edition. Example reactive currency pair trading app",
  "keywords": [
    "react",
    "fx",
    "spot",
    "cloud",
    "rxjs"
  ],
  "authors": [
    {
      "name": "Adaptive Consulting",
      "email": "@AdaptiveLimited"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaptiveConsulting/ReactiveTraderCloud.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run dev:cloud",
    "lint": "eslint --ext .js --ext .jsx src",
    "dev:local": "npm run dev -- --endpoint local",
    "dev:docker": "npm run dev -- --endpoint docker",
    "dev:cloud": "npm run dev -- --endpoint demo",
    "dev:openfin:local": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:local",
    "dev:openfin:cloud": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:cloud",
    "dev": "webpack-dev-server --progress --debug --config webpack.config.js --display-error-details",
    "test": "karma start ./karma.conf.js",
    "test:dev": "npm test -- --watch",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "deploy": "webpack --display-error-details",
    "deploy:prod": "export NODE_ENV=production && webpack",
    "openfin": "openfin -c config/openfin/local.app.json -l",
    "workbench": "start-storybook -p 9001"
  },
  "license": "MIT",
  "dependencies": {
    "animate.css": "3.5.2",
    "autobahn": "17.5.2",
    "classnames": "2.2.5",
    "d3": "4.9.1",
    "esp-js": "1.4.0",
    "esp-js-react": "0.1.6",
    "fixed-data-table": "0.6.4",
    "font-awesome": "4.7.0",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "numeral": "2.0.6",
    "nvd3": "1.8.6",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-nvd3": "0.5.7",
    "react-sizeme": "2.3.4",
    "react-sparklines": "1.6.0",
    "rx": "4.1.0",
    "ua-parser-js": "0.7.13",
    "ws": "3.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "2.35.3",
    "autoprefixer": "7.1.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.23.0",
    "chalk": "2.0.1",
    "classlist-polyfill": "1.2.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.2.0",
    "eslint-config-airbnb": "15.0.2",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.1.0",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "install": "0.10.1",
    "isparta-loader": "2.0.0",
    "jasmine-core": "2.6.4",
    "json-loader": "0.5.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "2.0.4",
    "minimist": "1.2.0",
    "node-sass": "4.5.3",
